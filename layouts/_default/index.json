{{- $.Scratch.Add "index" slice -}}
{{- range .Site.Pages -}}
    {{- $.Scratch.Add "index" (dict "Case Number" .Params.CaseDescriptionCaseNumber "Case Description" .Params.CaseDescription "Address" .Params.CircumstancesAddress "contents" .Plain "permalink" .Permalink) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
